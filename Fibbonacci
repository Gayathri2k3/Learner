class Solution {
    public  static int nthFibonacci(int n) {
        // code here
        int a = 0, b = 1;
        if (n == 0) return 0;
        if (n == 1) return 1;
        for  (int i = 2; i <= n; i++) {
            int temp = a + b;
            a = b;
            b = temp;
        }
        return b;
    }

    public static void main(String[] args) {
        int n = 10; 
        Solution sol=new Solution();
        System.out.println(sol.nthFibonacci(n));
    }
}
